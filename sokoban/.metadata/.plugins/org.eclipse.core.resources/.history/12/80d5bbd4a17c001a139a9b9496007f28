package pt.iscte.dcti.poo.sokoban.starter;

import pt.iul.ista.poo.utils.Point2D;

public class Target extends AbstractObjects {
	
	private SokobanGame game;
	
	public Target(Point2D position, SokobanGame game) {
		super(position);
		this.game = game;
	}


	@Override
	public String getName() {
		return "Target";
	}
	
	public boolean overlay() {
		for (AbstractObjects object: game.getObjects()) {
			if (object.getName()=="Box" && !getPosition().equals(object.getPosition()))
				return false;
		}
			
	}
		
}
