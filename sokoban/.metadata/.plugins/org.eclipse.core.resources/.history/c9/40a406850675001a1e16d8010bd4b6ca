package pt.iscte.dcti.poo.sokoban.starter;

import pt.iul.ista.poo.utils.Point2D;

public class Caixote extends Objetos implements ObjetosAnimados{
		
	public Caixote(Point2D position) {
		super(position);
	}


	@Override
	public String getName() {
		return "Caixote";
	}

	@Override
	public int getLayer() {
		return 2;
	}


	@Override
	public void move(Point2D newPosition) {
		getPosition().plus(new Point2D(newPosition));		
	}
	
}
